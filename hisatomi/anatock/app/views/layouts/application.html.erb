<!DOCTYPE html>
<html>
<head>
  <title>Anatock</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  
<div id="container">
  <div id="header">
    <%= render "shared/header" %>
  </div>
  <div id="content">
    <%= yield %>
  </div>
  <div id="footer">
    <%= render "shared/footer" %>
  </div>
</div>

</body>
</html>
